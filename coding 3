#include <iostream>
#include <stack>
using namespace std;

bool isMatching(char open, char close) {
    return (open == '(' && close == ')') ||
           (open == '{' && close == '}') ||
           (open == '[' && close == ']');
}

bool checkValid(string s) {
    stack<char> st;

    for (char c : s) {
        // jika simbol pembuka -> push
        if (c == '(' || c == '{' || c == '[') {
            st.push(c);
        }
        // jika simbol penutup -> cek kecocokan
        else if (c == ')' || c == '}' || c == ']') {
            if (st.empty()) return false; // tidak ada pasangan

            char top = st.top();
            st.pop();

            if (!isMatching(top, c)) return false; // tidak cocok
        }
    }

    return st.empty(); // jika kosong -> semua pasangan valid
}

int main() {
    string input;
    cout << "Input : ";
    getline(cin, input);

    if (checkValid(input))
        cout << "Output : Valid\n";
    else
        cout << "Output : Tidak Valid\n";

    return 0;
}
